{
    "name": "Wiktionary Popup",
    "description": "Sn√§lla funka",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": 
    [{
        "matches": ["*://*/*"],
        "js":["content.js"],
        "css": ["main.css"]
    },
    {
        "matches": ["*://*/*"],
        "match_about_blank": true,
        "all_frames": true,
        "css": ["frame.css"]
    }],
    "permissions": ["activeTab", "scripting", "storage"],
    "host_permissions": ["https://*/"]
  }